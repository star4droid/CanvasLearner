<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<FrameLayout
		android:id="@+id/frame"
		android:layout_width="match_parent"
		android:layout_height="150dp"
		android:layout_margin="5dp"
		android:padding="2dp"
		android:background="#DCEDC8"
		android:orientation="horizontal">
		<LinearLayout
			android:id="@+id/p1"
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:padding="8dp"
			android:background="#F57F17"
			android:orientation="horizontal" />
		<LinearLayout
			android:id="@+id/p2"
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:padding="8dp"
			android:background="#F57F17"
			android:orientation="horizontal" />
		<LinearLayout
			android:id="@+id/p3"
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:padding="8dp"
			android:background="#F57F17"
			android:orientation="horizontal"
			android:visibility="gone" />
		<LinearLayout
			android:id="@+id/p4"
			android:layout_width="25dp"
			android:layout_height="25dp"
			android:padding="8dp"
			android:background="#F57F17"
			android:orientation="horizontal"
			android:visibility="gone" />
		<LinearLayout
			android:id="@+id/fill_linear"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="2dp"
			android:background="#81C784"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:layout_gravity="left|bottom"
			android:visibility="gone">
			<ImageView
				android:id="@+id/imageview4"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:src="@drawable/fill_color"
				android:scaleType="center" />
			<CheckBox
				android:id="@+id/fill_color"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="1dp"
				android:paddingTop="4dp"
				android:paddingRight="1dp"
				android:paddingBottom="4dp"
				android:gravity="center_horizontal|center_vertical"
				android:textSize="12sp"
				android:textColor="#000000" />
		</LinearLayout>
		<ImageView
			android:id="@+id/full"
			android:layout_width="35dp"
			android:layout_height="35dp"
			android:background="#A5D6A7"
			android:src="@drawable/open"
			android:scaleType="fitCenter"
			android:layout_gravity="right|bottom" />
	</FrameLayout>
	<LinearLayout
		android:id="@+id/xy"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="5dp"
		android:padding="4dp"
		android:background="#DCEDC8"
		android:gravity="center_vertical"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/moveT"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="moveTo"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#000000" />
		<LinearLayout
			android:id="@+id/xp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="4dp"
			android:layout_marginTop="4dp"
			android:layout_marginBottom="4dp"
			android:padding="5dp"
			android:background="#FDD835"
			android:orientation="horizontal"
			android:layout_weight="1">
			<TextView
				android:id="@+id/xx"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="x:"
				android:textSize="12sp"
				android:textColor="#000000" />
			<EditText
				android:id="@+id/mx"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="0"
				android:textSize="12sp"
				android:textColor="#000000"
				android:textColorHint="#607D8B"
				android:singleLine="true"
				android:inputType="phone"
				android:imeOptions="actionNext" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/yp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_margin="4dp"
			android:padding="5dp"
			android:background="#FDD835"
			android:orientation="horizontal"
			android:layout_weight="1">
			<TextView
				android:id="@+id/yy"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="Y: "
				android:textSize="12sp"
				android:textColor="#000000" />
			<EditText
				android:id="@+id/my"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="0"
				android:textSize="12sp"
				android:textColor="#000000"
				android:textColorHint="#607D8B"
				android:singleLine="true"
				android:inputType="phone"
				android:imeOptions="actionNext"
				android:layout_weight="1" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/lxy"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="5dp"
		android:padding="4dp"
		android:background="#DCEDC8"
		android:gravity="center_vertical"
		android:orientation="horizontal">
		<TextView
			android:id="@+id/lineT"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="lineTo"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#000000" />
		<LinearLayout
			android:id="@+id/linear14"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear15"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/lxp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="4dp"
					android:layout_marginTop="4dp"
					android:layout_marginBottom="4dp"
					android:padding="5dp"
					android:background="#FDD835"
					android:orientation="horizontal"
					android:layout_weight="1">
					<TextView
						android:id="@+id/tx"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="x:"
						android:textSize="12sp"
						android:textColor="#000000" />
					<EditText
						android:id="@+id/lx"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="0"
						android:textSize="12sp"
						android:textColor="#000000"
						android:textColorHint="#607D8B"
						android:singleLine="true"
						android:inputType="phone"
						android:imeOptions="actionNext" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/lyp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="4dp"
					android:padding="5dp"
					android:background="#FDD835"
					android:orientation="horizontal"
					android:layout_weight="1">
					<TextView
						android:id="@+id/ty"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Y: "
						android:textSize="12sp"
						android:textColor="#000000" />
					<EditText
						android:id="@+id/ly"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="0"
						android:textSize="12sp"
						android:textColor="#000000"
						android:textColorHint="#607D8B"
						android:singleLine="true"
						android:inputType="phone"
						android:imeOptions="actionNext"
						android:layout_weight="1" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:orientation="horizontal"
				android:visibility="gone">
				<LinearLayout
					android:id="@+id/linear18"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="4dp"
					android:layout_marginTop="4dp"
					android:layout_marginBottom="4dp"
					android:padding="5dp"
					android:background="#FDD835"
					android:orientation="horizontal"
					android:layout_weight="1">
					<TextView
						android:id="@+id/textview10"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="x2:"
						android:textSize="12sp"
						android:textColor="#000000" />
					<EditText
						android:id="@+id/x2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="0"
						android:textSize="12sp"
						android:textColor="#000000"
						android:textColorHint="#607D8B"
						android:singleLine="true"
						android:inputType="phone"
						android:imeOptions="actionNext" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear17"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="4dp"
					android:padding="5dp"
					android:background="#FDD835"
					android:orientation="horizontal"
					android:layout_weight="1">
					<TextView
						android:id="@+id/txxx"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Y2: "
						android:textSize="12sp"
						android:textColor="#000000" />
					<EditText
						android:id="@+id/y2"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="0"
						android:textSize="12sp"
						android:textColor="#000000"
						android:textColorHint="#607D8B"
						android:singleLine="true"
						android:inputType="phone"
						android:imeOptions="actionNext"
						android:layout_weight="1" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linearc"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="2dp"
				android:orientation="horizontal"
				android:visibility="gone">
				<LinearLayout
					android:id="@+id/linear20"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="4dp"
					android:layout_marginTop="4dp"
					android:layout_marginBottom="4dp"
					android:padding="5dp"
					android:background="#FDD835"
					android:orientation="horizontal"
					android:layout_weight="1">
					<TextView
						android:id="@+id/textview12"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="x3:"
						android:textSize="12sp"
						android:textColor="#000000" />
					<EditText
						android:id="@+id/x3"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="0"
						android:textSize="12sp"
						android:textColor="#000000"
						android:textColorHint="#607D8B"
						android:singleLine="true"
						android:inputType="phone"
						android:imeOptions="actionNext" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear21"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="4dp"
					android:padding="5dp"
					android:background="#FDD835"
					android:orientation="horizontal"
					android:layout_weight="1">
					<TextView
						android:id="@+id/textview11"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Y3: "
						android:textSize="12sp"
						android:textColor="#000000" />
					<EditText
						android:id="@+id/y3"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="0"
						android:textSize="12sp"
						android:textColor="#000000"
						android:textColorHint="#607D8B"
						android:singleLine="true"
						android:inputType="phone"
						android:imeOptions="actionNext"
						android:layout_weight="1" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/angles"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="10dp"
		android:layout_marginTop="5dp"
		android:layout_marginRight="10dp"
		android:layout_marginBottom="5dp"
		android:padding="2dp"
		android:background="#DCEDC8"
		android:gravity="center_horizontal"
		android:orientation="vertical"
		android:visibility="gone">
		<LinearLayout
			android:id="@+id/linear23"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview13"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="StartAngle"
				android:textSize="12sp"
				android:textColor="#000000" />
			<SeekBar
				android:id="@+id/sa"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:max="360" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear24"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview14"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="SweepAngle"
				android:textSize="12sp"
				android:textColor="#000000" />
			<SeekBar
				android:id="@+id/swa"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:progress="180"
				android:max="360" />
		</LinearLayout>
		<CheckBox
			android:id="@+id/fm"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="forceMoveTo"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#000000" />
	</LinearLayout>
	<ScrollView
		android:id="@+id/linear9"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:orientation="vertical"
		android:layout_weight="1">
		<LinearLayout
			android:id="@+id/linear13"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<de.hdodenhof.circleimageview.CircleImageView
				android:id="@+id/circleimageview1"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:src="@drawable/java_icon"
				android:layout_gravity="center_horizontal"
				app:civ_border_width="3dp"
				app:civ_border_color="#008DCD"
				app:civ_circle_background_color="#FFFFFF"
				app:civ_border_overlay="true" />
			<TextView
				android:id="@+id/code"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="TextView"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#000000" />
		</LinearLayout>
	</ScrollView>
	<LinearLayout
		android:id="@+id/linear10"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:background="#FFE082"
		android:gravity="center_vertical"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/back"
			android:layout_width="35dp"
			android:layout_height="35dp"
			android:src="@drawable/next"
			android:scaleType="fitCenter"
			android:rotation="180"
			android:visibility="gone" />
		<TextView
			android:id="@+id/title"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="lineTo Tutorial"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#000000"
			android:layout_weight="1" />
		<ImageView
			android:id="@+id/next"
			android:layout_width="35dp"
			android:layout_height="35dp"
			android:src="@drawable/next"
			android:scaleType="fitCenter" />
	</LinearLayout>
</LinearLayout>
